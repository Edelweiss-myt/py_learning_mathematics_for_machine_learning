# LOESS
n = 10000
set.seed(1)
men = rnorm(n,176,7)
women = rnorm(n,162,7)
y = c(rep(0,n),rep(1,n))
x = round(c(men,women))
set.seed(5)
N = 250
ind = sample(length(y),N)
Y = y[ind]
X = x[ind]
height_lo <- loess(Y ~ X)
predict(height_lo, data.frame(X = 168))
xs = seq(160,178)
Pr =sapply(xs,function(x0) mean(Y[X==x0]))
plot(xs,Pr)
fitted=predict(height_lo,newdata=data.frame(X=xs))
lines(xs,fitted)
